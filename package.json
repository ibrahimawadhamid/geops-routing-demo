{
  "name": "geops-routing-demo",
  "description": "Sample component that uses geOps Routing API",
  "main": "build/index.js",
  "version": "1.0.0",
  "homepage": "https://routing-demo.geops.io/",
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.13.6",
    "@mui/styles": "5.13.2",
    "@mui/lab": "5.0.0-alpha.135",
    "@turf/combine": "^6.0.1",
    "@turf/nearest-point-on-line": "^6.0.2",
    "abortcontroller-polyfill": "^1.2.1",
    "canvas": "^2.11.0",
    "css-mediaquery": "^0.1.2",
    "is-ci": "3.0.1",
    "lodash": "^4.17.15",
    "mapbox-gl": "^1.13.1",
    "maplibre-gl": "^3.1.0",
    "mobility-toolbox-js": "2.0.0-beta.82",
    "ol": "7.4.0",
    "prop-types": "^15.7.2",
    "react": "18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.2.0",
    "react-id-generator": "^3.0.0",
    "react-redux": "^8.1.1",
    "react-spatial": "1.5.2",
    "recharts": "2.7.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "css-loader": "^6.8.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-cypress": "2.13.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "fixpack": "^4.0.0",
    "generact": "^0.4.0",
    "gh-pages": "^5.0.0",
    "husky": "8.0.3",
    "jest-canvas-mock": "^2.2.0",
    "jest-serializer-html": "^7.1.0",
    "lint-staged": "13.2.3",
    "prettier": "2.8.8",
    "react-scripts": "5.0.1",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.54.5",
    "style-loader": "^3.3.3",
    "stylelint": "15.9.0",
    "stylelint-config-recommended-scss": "12.0.0",
    "stylelint-config-standard": "33.0.0",
    "stylelint-scss": "5.0.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "cp": "generact --root src/Components",
    "deploy:ghpages": "gh-pages -d build",
    "eject": "react-scripts eject",
    "format": "prettier --write 'src/**/*.{js,jsx}' && eslint 'src/**/*.{js,jsx}' --fix && stylelint 'src/**/*.css' 'src/**/*.scss' --fix",
    "lint": "eslint 'src/**/*.{js,jsx}' && stylelint 'src/**/*.{css,scss}'",
    "prepare": "is-ci || husky install",
    "start": "ESLINT_NO_DEV_ERRORS=true  react-scripts start",
    "test": "react-scripts test --env=jsdom --transformIgnorePatterns \"/node_modules/(?!(@geops/react-ui|jsts|ol|ol-mapbox-style|ole|filter-obj|react-spatial|mobility-toolbox-js|quick-lru|split-on-first|query-string|decode-uri-component)/).*/\"",
    "up": "yarn upgrade-interactive --latest"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all",
    "not ie <= 11",
    "not android < 5"
  ],
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "jsx": true,
      "impliedStrict": true
    },
    "extends": [
      "airbnb",
      "airbnb/hooks",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "arrow-body-style": 0,
      "no-restricted-exports": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/forbid-prop-types": "Off",
      "prettier/prettier": "error"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "jest": {
    "coverageReporters": [
      "text",
      "html"
    ],
    "snapshotSerializers": [
      "jest-serializer-html"
    ]
  },
  "keywords": [
    "demo",
    "geops",
    "gis",
    "openlayers",
    "routing"
  ],
  "license": "MIT",
  "lint-staged": {
    "src/lang/*.json": [
      "yarn sort"
    ],
    "(src|__mocks__)/**/*.js": [
      "eslint --fix",
      "prettier --write",
      "yarn test --bail --findRelatedTests"
    ],
    "package.json": [
      "fixpack --sortToTop name --sortToTop description --sortToTop main --sortToTop version --sortToTop private --sortToTop homepage --sortToTop dependencies --sortToTop peerDependencies --sortToTop devDependencies --sortToTop scripts"
    ],
    "src/**/*.{css,scss}": [
      "stylelint --fix"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/geops/geops-routing-demo.git"
  },
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-recommended-scss"
    ],
    "rules": {
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "export"
          ]
        }
      ],
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "/^brandprimary/",
            "/gray/"
          ]
        }
      ]
    }
  }
}
